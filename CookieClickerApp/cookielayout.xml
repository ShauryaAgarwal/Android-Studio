<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/id_cookieConstraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#67EAEF"
    tools:context=".CookieActivity">

    <ImageView
        android:id="@+id/id_cookieImageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/id_shopPageIconImageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_cookiesPerSecondTextView"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/id_cookieCountTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:text="0 Cookies"
        android:textColor="#000000"
        android:textSize="28sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/id_cookiesPerSecondTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="0 Cookies Per Second"
        android:textColor="#4A4A4A"
        android:textSize="16sp"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="@+id/id_cookieCountTextView"
        app:layout_constraintStart_toStartOf="@+id/id_cookieCountTextView"
        app:layout_constraintTop_toBottomOf="@+id/id_cookieCountTextView" />

    <ImageView
        android:id="@+id/id_shopPageIconImageView"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginBottom="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/id_cookiePageIconImageView"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginEnd="18dp"
        android:layout_marginRight="18dp"
        app:layout_constraintBottom_toBottomOf="@+id/id_shopPageIconImageView"
        app:layout_constraintEnd_toStartOf="@+id/id_shopPageIconImageView"
        app:layout_constraintTop_toTopOf="@+id/id_shopPageIconImageView"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/id_itemsPageIconImageView"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginStart="18dp"
        android:layout_marginLeft="18dp"
        app:layout_constraintBottom_toBottomOf="@+id/id_shopPageIconImageView"
        app:layout_constraintStart_toEndOf="@+id/id_shopPageIconImageView"
        app:layout_constraintTop_toTopOf="@+id/id_shopPageIconImageView"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/id_shopPageIconTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="Shop"
        app:layout_constraintEnd_toEndOf="@+id/id_shopPageIconImageView"
        app:layout_constraintStart_toStartOf="@+id/id_shopPageIconImageView"
        app:layout_constraintTop_toBottomOf="@+id/id_shopPageIconImageView" />

    <TextView
        android:id="@+id/id_cookiePageIconTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="Cookie"
        app:layout_constraintEnd_toEndOf="@+id/id_cookiePageIconImageView"
        app:layout_constraintStart_toStartOf="@+id/id_cookiePageIconImageView"
        app:layout_constraintTop_toBottomOf="@+id/id_cookiePageIconImageView" />

    <TextView
        android:id="@+id/id_itemsPageIconTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="Items"
        app:layout_constraintEnd_toEndOf="@+id/id_itemsPageIconImageView"
        app:layout_constraintStart_toStartOf="@+id/id_itemsPageIconImageView"
        app:layout_constraintTop_toBottomOf="@+id/id_itemsPageIconImageView" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/id_cookiesFallingConstraintLayout"
        android:layout_width="match_parent"
        android:layout_height="360dp"
        app:layout_constraintBottom_toTopOf="@+id/id_shopPageIconImageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_cookiesPerSecondTextView">

        <ImageView
            android:id="@+id/id_cookiesFallingBackgroundImageView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:srcCompat="@tools:sample/avatars" />

        <ImageView
            android:id="@+id/id_cookiesFallingBackgroundImageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:srcCompat="@tools:sample/avatars" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>