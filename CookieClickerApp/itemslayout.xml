<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/id_itemsConstraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#8CF185">

    <ImageView
        android:id="@+id/id_cookiePageIconImageView3"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginEnd="18dp"
        android:layout_marginRight="18dp"
        app:layout_constraintBottom_toBottomOf="@+id/id_shopPageIconImageView3"
        app:layout_constraintEnd_toStartOf="@+id/id_shopPageIconImageView3"
        app:layout_constraintTop_toTopOf="@+id/id_shopPageIconImageView3"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/id_shopPageIconImageView3"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginBottom="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/id_itemsPageIconImageView3"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginStart="18dp"
        android:layout_marginLeft="18dp"
        app:layout_constraintBottom_toBottomOf="@+id/id_shopPageIconImageView3"
        app:layout_constraintStart_toEndOf="@+id/id_shopPageIconImageView3"
        app:layout_constraintTop_toTopOf="@+id/id_shopPageIconImageView3"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/id_cookiePageIconTextView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="Cookie"
        app:layout_constraintEnd_toEndOf="@+id/id_cookiePageIconImageView3"
        app:layout_constraintStart_toStartOf="@+id/id_cookiePageIconImageView3"
        app:layout_constraintTop_toBottomOf="@+id/id_cookiePageIconImageView3" />

    <TextView
        android:id="@+id/id_shopPageIconTextView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="Shop"
        app:layout_constraintEnd_toEndOf="@+id/id_shopPageIconImageView3"
        app:layout_constraintStart_toStartOf="@+id/id_shopPageIconImageView3"
        app:layout_constraintTop_toBottomOf="@+id/id_shopPageIconImageView3" />

    <TextView
        android:id="@+id/id_itemsPageIconTextView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="Items"
        app:layout_constraintEnd_toEndOf="@+id/id_itemsPageIconImageView3"
        app:layout_constraintStart_toStartOf="@+id/id_itemsPageIconImageView3"
        app:layout_constraintTop_toBottomOf="@+id/id_itemsPageIconImageView3" />

    <TextView
        android:id="@+id/id_itemsTitleTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Items"
        android:textColor="#000000"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/id_itemsDisplayCookieCountTotalTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:text="Cookie Count:"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ListView
        android:id="@+id/id_itemDisplayListView"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_itemsTitleTextView" />
</androidx.constraintlayout.widget.ConstraintLayout>